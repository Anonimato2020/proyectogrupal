<p id="notice"><%= notice %></p>

<h1>Alquilers</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Nhora</th>
      <th>Tpago</th>
      <th>Usuario</th>
      <th>Estacionamiento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alquilers.each do |alquiler| %>
      <tr>
        <td><%= alquiler.fecha %></td>
        <td><%= alquiler.hora %></td>
        <td><%= alquiler.nhora %></td>
        <td><%= alquiler.tpago %></td>
        <td><%= alquiler.usuario %></td>
        <td><%= alquiler.estacionamiento %></td>
        <td><%= link_to 'Show', alquiler %></td>
        <td><%= link_to 'Edit', edit_alquiler_path(alquiler) %></td>
        <td><%= link_to 'Destroy', alquiler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alquiler', new_alquiler_path %>
