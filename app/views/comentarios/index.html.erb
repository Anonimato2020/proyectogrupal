<p id="notice"><%= notice %></p>

<h1>Comentarios</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Puntuacion</th>
      <th>Descripcion</th>
      <th>Usuario</th>
      <th>Estacionamiento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comentarios.each do |comentario| %>
      <tr>
        <td><%= comentario.puntuacion %></td>
        <td><%= comentario.descripcion %></td>
        <td><%= comentario.usuario %></td>
        <td><%= comentario.estacionamiento %></td>
        <td><%= link_to 'Show', comentario %></td>
        <td><%= link_to 'Edit', edit_comentario_path(comentario) %></td>
        <td><%= link_to 'Destroy', comentario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comentario', new_comentario_path %>
